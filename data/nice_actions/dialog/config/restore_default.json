{
  "type": "minecraft:confirmation",
  "body": [
    {
      "type": "minecraft:plain_message",
      "contents": {
        "translate": "menu.nice_actions.restore.description",
        "fallback": "Proceeding will reset all configured values. Do you want to continue?"
      },
      "width": 1024
    },
    {
      "type": "minecraft:plain_message",
      "contents": {
        "translate": "menu.nice_actions.restore.warning",
        "fallback": "Restoring default values will in most cases not affect already existing mobs!",
        "color": "gray"
      },
      "width": 1024
    }
  ],
  "can_close_with_escape": true,
  "external_title": {
    "translate": "menu.nice_actions.restore.title",
    "fallback": "Restore Default Values"
  },
  "title": {
    "translate": "menu.nice_actions.config.main.title",
    "fallback": "Nice Actions: Admin Configuration"
  },
  "yes": {
    "label": {
      "translate": "option.nice_actions.confirm",
      "fallback": "Confirm"
    },
    "action": {
      "type": "run_command",
      "command": "function nice_actions:dialog/command_template/config/restore_default"
    }
  },
  "no": {
    "label": {
      "translate": "menu.mob_manager.back",
      "fallback": "Back"
    },
    "action": {
      "type": "show_dialog",
      "dialog": "nice_actions:config/main"
    }
  }
}