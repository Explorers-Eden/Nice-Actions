      },
      "tooltip": {
        "translate": "option.nice_actions.villagerdata.tooltip",
        "fallback": "Pull up details on the nearest Villager."
      },
      "action": {
        "type": "run_command",
        "command": "trigger nice_actions.dialog_trigger set 11"
      }
    },
    {
      "label": {
        "translate": "option.nice_actions.horsedata",
        "fallback": "Get Horse Info"
      },
      "tooltip": {
        "translate": "option.nice_actions.horsedata.tooltip",
        "fallback": "Pull up details on the nearest Horse."
      },
      "action": {
        "type": "run_command",
        "command": "trigger nice_actions.dialog_trigger set 12"
      }
    },
    {
      "label": {
        "translate": "option.nice_actions.stats",
