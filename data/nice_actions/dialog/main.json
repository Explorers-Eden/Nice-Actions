{
  "type": "minecraft:multi_action",
  "body": {
    "type": "minecraft:item",
    "item": {
      "id": "minecraft:writable_book",
      "components": {
        "minecraft:tooltip_display": {
          "hide_tooltip": true
        }
      }
    },
    "description": {
      "translate": "menu.nice_actions.description",
      "fallback": "Select An Action:"
    }
  },
  "can_close_with_escape": true,
  "external_title": {
    "translate": "menu.nice_actions.main.title.external",
    "fallback": "Actions"
  },
  "title": {
    "translate": "menu.nice_actions.main.title",
    "fallback": "Nice Actions"
  },
  "pause": false,
  "exit_action": {
    "label": {
      "translate": "mco.selectServer.close"
    }
  },
  "columns": 2,
  "actions": [
    {
      "label": {
        "translate": "option.nice_actions.coords_hud",
        "fallback": "Coordinates Hud"
      },
      "tooltip": {
        "translate": "option.nice_actions.coords_hud.tooltip",
        "fallback": "Switch the HUD that displays your coords on or off."
      },
      "action": {
        "type": "run_command",
        "command": "trigger nice_actions.dialog_trigger set 1"
      }
    },
    {
      "label": {
        "translate": "option.nice_actions.time_hud",
        "fallback": "Time/Date Hud"
      },
      "tooltip": {
        "translate": "option.nice_actions.time_hud.tooltip",
        "fallback": "Toggle the bossbar that shows the current time/date."
      },
      "action": {
        "type": "run_command",
        "command": "trigger nice_actions.dialog_trigger set 2"
      }
    },
    {
      "label": {
        "translate": "option.nice_actions.rtp",
        "fallback": "Random Teleport"
      },
      "tooltip": {
        "translate": "option.nice_actions.rtp.tooltip",
        "fallback": "Go for a random hop within the world."
      },
      "action": {
        "type": "run_command",
        "command": "trigger nice_actions.dialog_trigger set 3"
      }
    },
    {
      "label": {
        "translate": "option.nice_actions.tpspawn",
        "fallback": "Teleport To Spawn"
      },
      "tooltip": {
        "translate": "option.nice_actions.tpspawn.tooltip",
        "fallback": "Head back to spawn."
      },
      "action": {
        "type": "run_command",
        "command": "trigger nice_actions.dialog_trigger set 4"
      }
    },
    {
      "label": {
        "translate": "option.nice_actions.sethome",
        "fallback": "Set Your Home"
      },
      "tooltip": {
        "translate": "option.nice_actions.sethome.tooltip",
        "fallback": "Save this as your new private home point. The old one's gone."
      },
      "action": {
        "type": "run_command",
        "command": "trigger nice_actions.dialog_trigger set 5"
      }
    },
    {
      "label": {
        "translate": "option.nice_actions.tphome",
        "fallback": "Teleport To Your Home"
      },
      "tooltip": {
        "translate": "option.nice_actions.tphome.tooltip",
        "fallback": "Head back home instantly."
      },
      "action": {
        "type": "run_command",
        "command": "trigger nice_actions.dialog_trigger set 6"
      }
    },
    {
      "label": {
        "translate": "option.nice_actions.sit",
        "fallback": "Sit Down"
      },
      "tooltip": {
        "translate": "option.nice_actions.sit.tooltip",
        "fallback": "Grab a seat when you can. Left + right keys do the trick too."
